# Azure Communication Services Configuration
# Copy this file to .env and fill in your actual values

# Your ACS API Key (Obtain from Azure Portal > Keys)
# Example: A1a11aAaA1AaAA1aaA1Aa1A1AaAaA1AaAaa1aA11aA1aaaA1AaAaAaA1A1==
ACS_API_KEY=

# Your ACS Resource Endpoint (Obtain from Azure Portal > Keys)
# Example: https://<your-resource-name>.communication.azure.com/
ACS_ENDPOINT=

# Teams Tenant ID (Your Azure AD tenant ID where your app is registered)
# Example: 12345678-9abc-def0-1234-56789abcdef0
TEAMS_TENANT_ID=

# App ID (This is the Application (client) ID from your Azure AD app registration)
# Example: 12345678-9abc-def0-1234-56789abcdef0
APP_ID=

# Object ID (This is the Object ID of your EntraId User)
# Example: 12345678-9abc-def0-1234-56789abcdef0
USER_OBJECT_ID=

# Set to true for local development/testing
IsLocal=false